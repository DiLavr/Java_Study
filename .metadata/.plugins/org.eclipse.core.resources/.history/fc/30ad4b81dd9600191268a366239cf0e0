package chapter_21_Stacks_Queues;
import java.util.*;
public class TestIterator_rus {
public static void main(String [] args) {
	ArrayList<String> states = new ArrayList<String>();
	states.add("German");
	 states.add("France");
     states.add("Italy");
     states.add("Spain");
     
     ListIterator<String> listIter = states.listIterator();
     
    	while(listIter.hasNext()) {
    		System.out.println(listIter.next());
    	}
    	  // —?–µ–π—á–∞—? —Ç–µ–∫—É—â–∏–π —?–ª–µ–º–µ–Ω—Ç - –ò—?–ø–∞–Ω–∏—?
        // –∏–∑–º–µ–Ω–∏–º –∑–Ω–∞—á–µ–Ω–∏–µ —?—Ç–æ–≥–æ —?–ª–µ–º–µ–Ω—Ç–∞
        listIter.set("–ü–æ—Ä—Ç—É–≥–∞–ª–∏—?");
        // –ø—Ä–æ–π–¥–µ–º—?—? –ø–æ —?–ª–µ–º–µ–Ω—Ç–∞–º –≤ –æ–±—Ä–∞—Ç–Ω–æ–º –ø–æ—Ä—?–¥–∫–µ
        while(listIter.hasPrevious()){
         
            System.out.println(listIter.previous());
        } 
    	 
    	 
     }
		
     }
